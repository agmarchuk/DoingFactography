@using RDFEngine

@if (focusrecord != null)
{
    if (true || focusrecord.Tp == "http://fogid.net/o/person")
    {
        <MagBlazor.Components.ViewPortraitPerson focusrecord="@focusrecord" />
    }
    else if (focusrecord.Tp == "http://fogid.net/o/org-sys")
    {
        <MagBlazor.Components.ViewPortraitOrgsys focusrecord="@focusrecord" />
    }
}

@code {
    [Parameter]
    public RRecord focusrecord { get; set; }
}
